{
  "expo": {
    "name": "readsy",
    "slug": "readsy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "readsy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "./App.tsx",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "dark": {
          "backgroundColor": "#222E3A",
          "foregroundImage": "./assets/images/adaptive-icon-dark.png",
          "monochromeImage": "./assets/images/adaptive-icon-dark.png"
        },
        "backgroundColor": "#F7F4EB",
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "monochromeImage": "./assets/images/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hdmi.readsy"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F7F4EB",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#222E3A"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "33d418ad-9105-454c-8813-0a07ddd30349"
      }
    }
  }
}
