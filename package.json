{
  "name": "end-term-project-books",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db-up": "docker-compose -f database/docker-compose.yaml up -d"
  },
  "devDependencies": {
    "pnpm": "^10.20.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.1.1",
      "react-dom": "^19.1.1"
    }
  }
}
